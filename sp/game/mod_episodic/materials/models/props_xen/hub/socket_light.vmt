"VertexLitGeneric"
{	
	$basetexture "models/props_xen/hub/socket_light"
	$bumpmap "models/props_xen/hub/socket_light_nm"
	"$normalmapalphaenvmapmask" "1"
	"$envmap" "env_cubemap"
	"$phongboost" "7"
	
	"$envmaptint"			"[0.4 0.4 0.4]"
	$phong 1
        $phongexponent 50
        $phongtint "[1 1 1]"
        $halflambert 0
        $phongfresnelranges "[.4 .8 30]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "50"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting
"$phongexponenttexture" "models\props_xen\hub\socket_light_exp"
    "$model" 1
}