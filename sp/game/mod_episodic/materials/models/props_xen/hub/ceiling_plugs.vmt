"VertexLitGeneric"
{	
	$model 1
	$basetexture "models/props_xen/hub/ceiling_plugs"
	$bumpmap "models/props_xen/hub/ceiling_plugs_nm"
"$normalmapalphaenvmapmask" "1"
"$envmap" "env_cubemap"

"$envmaptint"			"[0.3 0.3 0.3]"
"$selfillum" 1
$phong 1
        $phongexponent 20
        $phongboost 1
        $halflambert 0
"$phongfresnelranges"	"[.6 0.8 2]"

// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "20"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".3"	               // Boost for ambient cube component of rim lighting

}