"VertexLitGeneric"
{	
	$model 1
	$basetexture "models/props_xen/interloper/int_c_laser_off"
	$bumpmap "models/props_xen/interloper/int_c_laser_nm"
"$normalmapalphaenvmapmask" "1"
"$envmap" "env_cubemap"



"$phongalbedotint" "1"
	"$envmaptint"			"[0.2 0.2 0.2]"
"$selfillum" 1
$phong 1
        $phongexponent 7
        $phongboost 2
        $phongtint "[1 1 1]"
