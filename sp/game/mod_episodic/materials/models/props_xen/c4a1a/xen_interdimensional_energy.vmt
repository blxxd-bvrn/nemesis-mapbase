"UnlitGeneric"
{
	"$basetexture" "models/props_xen/c4a1a/xen_interdimensional_energy"
	"$additive" "1"
	//"$vertexalpha" "1"
	//"$vertexcolor" 1
	"%keywords" "bms"
	"Proxies"
	{
		"AnimatedTexture"
		{
			"animatedtexturevar" "$basetexture"
			"animatedtextureframenumvar" "$frame"
			"animatedtextureframerate" 30
		}
		"TextureScroll"
		{
			"texturescrollvar" "$baseTextureTransform"
			"texturescrollrate" 1.2
			"texturescrollangle" 180
		}
	}
	"$model" 1 
}
