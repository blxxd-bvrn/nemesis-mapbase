"VertexLitGeneric"
{

    "$basetexture" 	"models\props_lambdabunker\waterfall"
    	"$model"	"1"
	"$translucent"  "1"
	"$bumpmap" 	"models\props_lambdabunker\waterfall_nm"
	"$surfaceprop" "slime"

	"$envmap"	"env_cubemap"
        "$normalmapalphaenvmapmask" "1"
	"$envmaptint" "[1 1 1]"
	"$phong" "1"
	"$phongexponent" "80"
	"$phongboost"	"10"
	  $phongfresnelranges "[.4 .8 30]"
	"$phongtint" "[.42 .6 .9]"
	"$halflambert" 1
	"$multipass" 1


	// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "35"		// Exponent for phong component of rim lighting
	"$rimlightboost" "5"			// Boost for ambient cube component 


 	"$nocull" "1"


	"Proxies"

	{

		"TextureScroll"
		{
			"texturescrollvar" "$baseTextureTransform"
		
			"texturescrollrate" 3.5
			
			"texturescrollangle" 270.00
		}

	}
	
}
