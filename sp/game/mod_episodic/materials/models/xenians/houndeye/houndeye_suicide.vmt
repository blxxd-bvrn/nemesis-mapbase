"VertexLitGeneric"
{
	"$baseTexture" "models\xenians\houndeye/houndeye_suicide_d"
	"$bumpmap" "models\xenians\houndeye/houndeye_suicide_n"
	$detail detail\noise_detail_01 // e.g. for brickwork, concrete...
	$detailscale 7.74
	$detailblendfactor 0.8
	$detailblendmode 0
	$selfillum 1
	$selfillummask models\xenians\houndeye\houndeye_suicide_illum

	"$phong" "1"
	"$phongboost" "2"
	"$phongexponent" 13
	"$phongfresnelranges" "[1 .4 0]"
	"$phongwarptexture"  "models/effects/phongwarp_library/iridescence011"
	
	// Rim lighting parameters
	"$rimlight" "1"				// To enable rim lighting (requires phong)
	"$rimlightexponent" "60"             // Exponent for phong component of rim lighting
	"$rimlightboost" ".5"	               // Boost for ambient cube component of rim lighting
	
	$emissiveBlendEnabled 1
	"$emissiveBlendTexture" "models\props_xen\glow_red6"
	$emissiveBlendFlowTexture "models\props_xen\glow_red5"
	$emissiveBlendBaseTexture models\xenians\houndeye\houndeye_suicide_exp
	//$emissiveBlendTint "[0.3 0 0]"
	"$emissiveBlendScrollVector" "[0.8 0.4]"
	$emissiveBlendStrength 1
	


    "$model" 1
}